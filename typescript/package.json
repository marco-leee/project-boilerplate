{
  "name": "@intellistixman",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "bun run lint && bun run format:check && bun run type-check"
  },
  "dependencies": {
    "@intellistixman/algorithms": "workspace:*",
    "@intellistixman/crypto": "workspace:*",
    "@intellistixman/data-structures": "workspace:*",
    "@intellistixman/os": "workspace:*",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.44.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
